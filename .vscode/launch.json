    "version": "0.2.0",
    "configurations": [
        {
            "name": "gdb debugger",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/jiang/research-project/dbms-testing/sqlsmith-1.2.1/sqlsmith",
            "args": ["--sqlite=test123.db", 
                    "--exclude-catalog", 
                    "--max-queries=1", 
                    "--verbose", 
                    "--dry-run"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test",
            "environment": [],
            "console": "externalTerminal",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}